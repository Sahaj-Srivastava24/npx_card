{
  "name": "mad1ad",
  "version": "1.2.0",
  "description": "A CLI tool to show my profile information.",
  "main": "dist/index.js",
  "scripts": {
    "watch": "nodemon",
    "dev": "npx tsc && node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean:all": "rm -r dist && rm -r node_modules && rm package-lock.json",
    "clean:compile": "rm -r dist && rm -r node_modules && rm package-lock.json",
    "style:lint": "eslint . --ext .ts",
    "style:prettier": "prettier --check \"**/*.+(js|ts|json)\"",
    "style:prettier-write": "prettier --write \"**/*.+(js|ts|json)\"",
    "style:all": "npm run style:lint && npm run style:prettier",
    "prepare": "husky install",
    "package": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sahaj-Srivastava24/npx_card.git"
  },
  "keywords": [
    "card",
    "npm",
    "npm card",
    "npx",
    "npx card",
    "business card"
  ],
  "author": "Sahaj Srivastava <srivastavasahaj24@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Sahaj-Srivastava24/npx_card/issues"
  },
  "homepage": "https://github.com/Sahaj-Srivastava24/npx_card#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@types/boxen": "^3.0.1",
    "@types/chalk": "^2.2.0",
    "@types/clear": "^0.1.2",
    "@types/clui": "^0.3.1",
    "@types/inquirer": "^9.0.1",
    "boxen": "^4.2.0",
    "chalk": "^4.1.2",
    "clear": "^0.1.0",
    "cli-format": "^3.0.9",
    "clui": "^0.3.6",
    "download": "^8.0.0",
    "husky": "^8.0.1",
    "inquirer": "^8.1.0",
    "open": "^8.4.0",
    "pkg": "^5.8.0"
  },
  "pkg": {
    "scripts": "dist/**/*"
  },
  "files": [
    "dist/**/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "list-staged": {
    "*": "lint && format"
  }
}
